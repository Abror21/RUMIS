{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Common": {
    "TicketPassword": "__REQUIRED__",
    "TicketReplyUrl": "__REQUIRED__/_api/auth/ticket",
    "SignOutRedirectUrl": "__REQUIRED__",
    "ErrorRedirectUrl": "__REQUIRED__/auth/login?error=true",
    "AdfsEnabled": true,
    "WindowsEnabled": false
  },
  "Adfs": {
    "BaseUrl": "__OPTIONAL__",
    "Wtrealm": "__OPTIONAL__",
    "MetadataAddress": "__OPTIONAL__"
  }
}
